<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="433.0" prefWidth="639.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.ProductoController">
   <children>
      <Button fx:id="btnAgregar" layoutX="60.0" layoutY="394.0" mnemonicParsing="false" onAction="#agregarProducto" prefHeight="25.0" prefWidth="149.0" text="Agregar Producto" />
      <TableView fx:id="tblProductos" layoutX="34.0" layoutY="67.0" prefHeight="306.0" prefWidth="589.0">
        <columns>
          <TableColumn fx:id="colCategoria" prefWidth="215.0" text="Categoria" />
          <TableColumn fx:id="colProducto" prefWidth="237.0" text="Producto" />
            <TableColumn fx:id="colValor" prefWidth="123.0" text="Valor" />
        </columns>
      </TableView>
      <Button fx:id="btnModificar" layoutX="241.0" layoutY="394.0" mnemonicParsing="false" onAction="#modificar" prefHeight="25.0" prefWidth="169.0" text="Modificar Producto" />
      <Button fx:id="btnEliminar" layoutX="444.0" layoutY="394.0" mnemonicParsing="false" onAction="#eliminar" prefHeight="25.0" prefWidth="156.0" text="Eliminar Producto" />
      <Label layoutX="40.0" layoutY="29.0" text="Categoria" />
      <TextField fx:id="txtFiltrarCategoria" layoutX="154.0" layoutY="25.0" onKeyReleased="#filtrarCategoria" />
   </children>
</AnchorPane>
